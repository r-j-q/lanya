

/* 地图图标  begin */
.map {
	height: 31px;
	color: rgba(255, 255, 255, 1);
	background-color: rgba(56, 56, 56, 1);
	border-radius: 15px;
	line-height: 31px;
	text-align: center;
	position: absolute;
	top: 14px;
	left: 18px;
	padding: 0 10px;
}
.map_icon {
	font-family: iconfont;
	color: #fff;
	font-size: 19px;
}
.map_text {
	color: #ffffff;
	font-size: 12px;
	line-height: 20px;
	margin-left: 2px;
	vertical-align: text-bottom;
}
/* 地图图标  end */

